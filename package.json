{
  "name": "gterm",
  "version": "0.1.0",
  "description": "GUIを用いたコマンドのビルドができるターミナルエミュレータ",
  "main": "init.js",
  "author": "282Haniwa",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "develop": "gulp develop",
    "pack:mac": "gulp pack-mac",
    "pack:win": "gulp pack-win",
    "pack:linux": "gulp pack-linux",
    "lint": "eslint --ext .js,.jsx ./ --cache && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint --ext .js,.jsx ./ --cache --fix && echo \"eslint: no lint errors\"",
    "clean": "rimraf build dist node_modules"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.3.0",
    "clsx": "^1.0.4",
    "immutable": "^4.0.0-rc.12",
    "node-pty": "0.9.0-beta27",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "uuid": "^3.3.3",
    "xterm": "^4.1.0",
    "xterm-addon-fit": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-sync": "^2.26.7",
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gulp": "^4.0.2",
    "gulp-inject-string": "^1.1.2",
    "husky": "^3.0.7",
    "lint-staged": "^9.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.35.0",
    "webpack-command": "^0.4.2",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.jsx": [
      "eslint --fix",
      "git add"
    ]
  }
}
